{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2022",
    "lib": ["es2021", "dom", "dom.iterable"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": ["@cloudflare/workers-types"]
  },
  "exclude": ["example"]
}
